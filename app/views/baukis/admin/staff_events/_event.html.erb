<tr>
  <% unless @staff_member %>
      <td>
        <%= link_to(event.member.family_name + event.member.given_name,
                    baukis_admin_staff_member_staff_events_path(event.member) ) %>
      </td>
  <% end %>
  <td><%= event.description %></td>
  <td class="date">
    <%= event.occurred_at.strftime('%Y/%m/%d %H:%M:%S') %>
  </td>
</tr>