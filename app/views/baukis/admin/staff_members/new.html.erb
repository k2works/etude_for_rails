<% @title = '職員の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @staff_member, url: baukis_admin_staff_members_path do |f| %>
      <%= render 'form', f: f %>
      <div class="buttons">
        <%= f.submit '登録' %>
        <%= link_to 'キャンセル', :baukis_admin_staff_members %>
      </div>
  <% end %>
</div>