<% @title = '職員アカウントの編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @staff_member, url:baukis_admin_staff_member_path do |f| %>
      <%= render 'form', f: f %>
      <div class="buttons">
        <%= f.submit '更新' %>
        <%= link_to 'キャンセル', :baukis_admin_staff_members %>
      </div>
  <% end %>
</div>