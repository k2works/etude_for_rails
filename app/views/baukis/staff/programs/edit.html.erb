<% @title = 'プログラムの編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @program_form, as: 'form', url: baukis_staff_program_path(@program_form.program) do |f| %>
      <%= render 'form', f: f%>
      <div class="buttons">
        <%= f.submit '更新'%>
        <%= link_to 'キャンセル', :baukis_staff_programs %>
      </div>
  <% end %>
</div>